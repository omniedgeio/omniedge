<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate
    xmlns="http://schemas.microsoft.com/msix/packaging/msixpackagingtool/template/2022">
    <Settings
        AllowTelemetry="false"
        ApplyACLsToPackageFiles="true"
        GenerateCommandLineFile="true"
        AllowPromptForPassword="false">
    </Settings>
    <Installer
        Path="omniedge"
        Arguments=""
        InstallLocation="C:\Program Files\OmniEdge\OmniEdge">
    </Installer>
    <PackageInformation
        PackageName="OmniEdge"
        PackageDisplayName="OmniEdge"
        PublisherName="CN=OmniEdge"
        PublisherDisplayName="OmniEdge"
        Version="0.1.0.0"
        PackageDescription="OmniEdge VPN Client">
        <Capabilities>
            <Capability Name="runFullTrust" />
            
        </Capabilities>
        <Applications>
            <Application
                Id="io.omniedge.desktop"
                Description="OmniEdge VPN Client"
                DisplayName="OmniEdge"
                ExecutableName="omniedge"
                EntryPoint="Windows.FullTrustApplication">
                
            </Application>
        </Applications>
        <Resources>
            <Resource Language="en-us" />
        </Resources>
        <Dependencies>
            <TargetDeviceFamily Name="Windows.Desktop" MinVersion="10.0.17763.0" MaxVersionTested="10.0.19041.0" />
        </Dependencies>
        <Properties>
            <Framework>false</Framework>
            <DisplayName>OmniEdge</DisplayName>
            <PublisherDisplayName>OmniEdge</PublisherDisplayName>
            <Description>OmniEdge VPN Client</Description>
            <Logo>Assets\AppIcon.png</Logo>
        </Properties>
    </PackageInformation>
    <SaveLocation PackagePath="omniedge.msix" />
    <PackageIntegrity>
        <CertificatePath></CertificatePath>
    </PackageIntegrity>
</MsixPackagingToolTemplate>
